{
  "plugins": [ 
  "@trivago/prettier-plugin-sort-imports",
  "prettier-plugin-tailwindcss"
],
  "trailingComma": "all",
  "printWidth": 80,
  "tabWidth": 2,
  "arrowParens": "always",
  "semi": true,
  "singleQuote": false,
  "useTabs": false,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/*/(.*)$",
    "^[../]",
    "^[./]",
    "^import type(.*)from(.*)$"
  ],
  "importOrderSeparation": true
}
